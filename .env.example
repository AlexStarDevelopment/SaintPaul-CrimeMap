# Database
MONGODB_URI=mongodb://localhost:27017/saintpaul-crime-map

# Authentication
NEXTAUTH_URL=http://localhost:3001
NEXTAUTH_SECRET=your-secret-key-here
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Cache Configuration
CACHE_WARMING_ENABLED=true
CACHE_WARMING_DELAY=10000
CACHE_WARMING_INTERVAL=3600000
CACHE_BACKGROUND_REFRESH_INTERVAL=1800000
CACHE_DEFAULT_TTL=300000
CACHE_MAX_ENTRIES=1000
CACHE_CLEANUP_INTERVAL=60000
CACHE_HISTORICAL_TTL=31536000000

# Rate Limiting (Upstash Redis)
UPSTASH_REDIS_REST_URL=your-upstash-redis-url
UPSTASH_REDIS_REST_TOKEN=your-upstash-redis-token

# Subscription (Stripe)
STRIPE_SECRET_KEY=sk_test_your_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
STRIPE_SUPPORTER_PRICE_ID=price_xxx
STRIPE_PRO_PRICE_ID=price_xxx

# Environment
NODE_ENV=development